{
  "name": "mko-custom-sample",
  "version": "0.1.2",
  "description": "Sample use mko-custom",
  "main": "main/index.ts",
  "scripts": {
    "start": "live-server app",
    "build": "jspm bundle-sfx ./app/src/index.ts app/build/bundel.js --skip-source-maps --minify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MaciejMisztal/mko-custom-sample.git"
  },
  "keywords": [
    "mko",
    "knockoutjs",
    "custom-element"
  ],
  "author": "Maciej Misztal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MaciejMisztal/mko-custom-sample/issues"
  },
  "homepage": "https://github.com/MaciejMisztal/mko-custom-sample#readme",
  "devDependencies": {
    "jspm": "^0.16.33",
    "live-server": "^0.9.0"
  },
  "jspm": {
    "directories": {
      "baseURL": "app"
    },
    "dependencies": {
      "knockout": "github:knockout/knockout@^3.4.0",
      "mko-custom": "github:MaciejMisztal/mko-custom@^0.1.1",
      "text": "github:systemjs/plugin-text@^0.0.7",
      "ts": "github:frankwallis/plugin-typescript@^4.0.6"
    },
    "devDependencies": {
      "typescript": "npm:typescript@^1.8.10"
    }
  }
